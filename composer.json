{
    "name": "videni/casbin",
    "type": "library",
    "description": "Integrate php-casbin into Symfony",
    "keywords": ["symfony", "casbin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vidy Videni"
        }
    ],
    "require": {
        "php": ">7.2",
        "casbin/casbin": "^2.1"
    },
    "repositories": [
        {
            "url": "https://github.com/videni/casbin.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "psr-4": {
            "Videni\\Casbin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Videni\\Casbin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
