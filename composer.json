{
    "name": "videni/symfony-casbin",
    "type": "symfony-bundle",
    "description": "Integrate php-casbin into Symfony",
    "keywords": ["symfony", "bundle", "casbin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vidy Videni"
        }
    ],
    "require": {
        "php": ">5.3.0",
        "casbin/casbin": "^1.0@dev",
        "symfony/framework-bundle": "~2.1|~3.0|^4.0"
    },
    "repositories": [
        {
            "url": "https://github.com/videni/php-casbin.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "psr-4": {
            "Videni\\Bundle\\CasbinBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Videni\\Bundle\\CasbinBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
